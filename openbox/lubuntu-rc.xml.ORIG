     <command>lxtask</command>
    </action>
  </keybind>
<!-- Fast access to Terminal -->
  <keybind key="W-t">
    <action name="Execute">
     <command>lxterminal</command>
    </action>
  </keybind>
<!-- Fast access to filemanager -->
  <keybind key="C-A-D">
    <action name="Execute">
     <startupnotify>
     <enabled>true</enabled>
     <name>PCManFM</name>
     </startupnotify>
     <command>pcmanfm</command>
    </action>
  </keybind>
<!-- Keybinding for PrintScreen Key -->
  <keybind key="Print">
